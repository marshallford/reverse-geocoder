{
  "name": "node-esnext-boilerplate",
  "version": "1.0.0",
  "description": "An opinionated, but ideally up to date NodeJS project boilerplate with ESnext support",
  "main": "src/server.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint src",
    "precommit": "eslint src",
    "dev": "nodemon --exec babel-node src/server.js",
    "prestart": "rimraf dist && babel src --out-dir dist",
    "start": "node dist/server.js",
    "test": "mocha --reporter spec --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marshallford/node-esnext-boilerplate.git"
  },
  "author": "Marshall Ford <inbox@marshallford.me> (https://marshallford.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marshallford/node-esnext-boilerplate/issues"
  },
  "homepage": "https://github.com/marshallford/node-esnext-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-root-import": "^3.2.2",
    "chai": "^3.5.0",
    "eslint": "^2.11.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "husky": "^0.11.4",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "body-parser": "^1.15.1",
    "express": "^4.13.4",
    "limiter": "^1.1.0",
    "lodash": "^4.13.1",
    "pg": "^5.1.0",
    "redis": "^2.6.1"
  }
}
