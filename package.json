{
  "name": "reverse-geocoder",
  "version": "2.0.0",
  "description": "An opinionated, but ideally up to date NodeJS project boilerplate with ESnext support",
  "main": "src/server.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "private": true,
  "scripts": {
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch",
    "lint": "better-npm-run lint",
    "dev": "better-npm-run dev",
    "prestart": "better-npm-run prestart",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "test": {
      "command": "mocha --reporter spec --compilers js:babel-core/register",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:watch": {
      "command": "mocha --watch --reporter spec --compilers js:babel-core/register",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "lint": {
      "command": "eslint src test",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node src/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prestart": {
      "command": "rimraf dist && babel --s inline --copy-files src -d dist",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    },
    "start": {
      "command": "node dist/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marshallford/reverse-geocoder.git"
  },
  "author": "Marshall Ford <inbox@marshallford.me> (https://marshallford.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marshallford/reverse-geocoder/issues"
  },
  "homepage": "https://github.com/marshallford/reverse-geocoder#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^2.0.0-alpha.12",
    "babel-root-import": "^4.1.8",
    "better-npm-run": "0.0.15",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "express": "^4.15.3",
    "limiter": "^1.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "pg": "^6.2.4",
    "redis": "^2.7.1",
    "winston": "^2.3.1"
  }
}
